<p id="notice"><%= notice %></p>

<h1>Processos</h1>

<table>
  <thead>
    <tr>
      <th>Vara</th>
      <th>Numero processo</th>
      <th>Natureza</th>
      <th>Partes</th>
      <th>Urgente</th>
      <th>Arbitramento</th>
      <th>Assistente social</th>
      <th>Data entrada</th>
      <th>Data saida</th>
      <th>Data agendamento</th>
      <th>Status</th>
      <th>Observacao</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @processos.each do |processo| %>
      <tr>
        <td><%= processo.vara %></td>
        <td><%= processo.numero_processo %></td>
        <td><%= processo.natureza %></td>
        <td><%= processo.partes %></td>
        <td><%= processo.urgente %></td>
        <td><%= processo.arbitramento %></td>
        <td><%= processo.assistente_social %></td>
        <td><%= processo.data_entrada %></td>
        <td><%= processo.data_saida %></td>
        <td><%= processo.data_agendamento %></td>
        <td><%= processo.status %></td>
        <td><%= processo.observacao %></td>
        <td><%= link_to 'Show', processo %></td>
        <td><%= link_to 'Edit', edit_processo_path(processo) %></td>
        <td><%= link_to 'Destroy', processo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Processo', new_processo_path %>
